<h1>All Notes</h1>

<% @notes.each do |note| %>
<article class="note">
  <h1><a href="/notes/<%= note.slug %>"><%= note.title %></a></h1>
  <%= note.content %>
  <p class="post_date">Posted by <a href="/users/<%= note.user.slug %>"><%= note.user.username %></a> at <%= note.created_at.strftime("%B %d, %Y") %></p>
</article>
<% end %>